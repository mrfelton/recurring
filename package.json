{
  "name": "recurring",
  "description": "a recurly v2 api client for node.js",
  "version": "2.2.0",
  "bugs": "http://github.com/ceejbot/recurring/issues",
  "contributors": [
    "C J Silverio <ceejceej@gmail.com> (http://ceejbot.github.io/)",
    "Julian Gautier <julian.gautier@alumni.neumont.edu> (http://www.juliangautier.com/)",
    "Simon Tabor",
    "T J @surttour",
    "Peter Shugar",
    "Mark Garlanger @mgarlanger",
    "Tom Kirkpatrick @mrfelton"
  ],
  "config": {
    "blanket": {
      "pattern": "lib",
      "data-cover-never": [
        "node_modules",
        "test"
      ],
      "output-reporter": "spec"
    }
  },
  "dependencies": {
    "throttled-request": "~0.1.1",
    "data2xml": "~0.5.0",
    "lodash": "~3.7.0",
    "node-uuid": "~1.4.3",
    "qs": "~2.4.1",
    "request": "^2.55.0",
    "xml2js": "~0.4.8"
  },
  "devDependencies": {
    "async-iterators": "~0.2.2",
    "blanket": "~1.1.6",
    "debug": "^2.2.0",
    "jscs": "~1.12.0",
    "jshint": "~2.7.0",
    "mocha": "~2.2.4",
    "mocoverage": "~1.0.0",
    "must": "~0.12.0",
    "patched-blanket": "~1.0.1"
  },
  "directories": {
    "test": "test"
  },
  "keywords": [
    "recurly",
    "payments"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "readmeFilename": "README.md",
  "repository": {
    "type": "git",
    "url": "git://github.com/ceejbot/recurring.git"
  },
  "scripts": {
    "test": "mocha --check-leaks --timeout 5000  --slow 1000 --ui exports --require patched-blanket -R mocoverage",
    "lint": "jshint lib/* test/* && jscs lib/* test/*"
  }
}
